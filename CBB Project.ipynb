    "# create an empty list to store the results of each KNN model with different values of k\n",
    "results = []\n",
    "\n",
    "# loop through values of k from 1 to 199\n",
    "for k in range(1, 200):\n",
    "    # fit a KNN model with k neighbors using the training data\n",
    "    knn = KNeighborsClassifier(n_neighbors=k).fit(X_train, y_train)\n",
    "    \n",
    "    # predict the classes of the test set using the fitted model\n",
    "    y_pred = knn.predict(X_test)\n",
    "    \n",
    "    # store the accuracy score of the predictions in the results list as a dictionary\n",
    "    results.append({\n",
    "        'k': k,\n",
    "        'accuracy': accuracy_score(y_test, y_pred)\n",
    "    })\n",
    "\n",
    "# convert the results list to a pandas DataFrame\n",
    "results = pd.DataFrame(results)\n",
    "\n",
    "# generate a line plot of the accuracy scores for different values of k\n",
    "results.plot(x=\"k\", y=\"accuracy\", kind=\"line\")\n",
    "print(results.max())"
