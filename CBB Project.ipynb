   "source": [
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=.25, random_state = 0 )\n",
    "scaler = StandardScaler()\n",
    "X_train_scaled = scaler.fit_transform(X_train)\n",
    "X_test_scaled = scaler.transform(X_test)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "f801b932",
   "metadata": {},
   "outputs": [],
   "source": [
    "# create a dictionary of parameters to search over\n",
    "parameters = {\"n_neighbors\": range(1, 200)}\n",
    "\n",
    "# perform a grid search to find the best hyperparameters for the KNN model\n",
    "grid_search = GridSearchCV(KNeighborsClassifier(), parameters, cv=5)\n",
    "grid_search.fit(X_train_scaled, y_train)\n",
    "\n",
    "# get the cross-validation results from the grid search and store the mean test scores in a list\n",
    "gr = grid_search.cv_results_\n",
    "a = list(gr[\"mean_test_score\"])\n",
    "\n",
    "# store a list of values of k from 1 to 199\n",
    "b = list(range(1, 200))\n",
    "\n",
    "# find the index of the highest mean test score and use it to find the best value of k\n",
    "a_len = np.argmax(a)\n",
    "k_best = b[a_len]\n"
   ]
  },
